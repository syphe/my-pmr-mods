                        <widget name="revs" type="palShaderslider" sParam="0_0_3" r="1.0" g="1.0" b="1.0" rh="1.0" gh="0.05" bh="0.05" minValue="0" maxValue ="8000" highThreshold="6600" rateTransT="1400" vehicleParam="revs"/>
