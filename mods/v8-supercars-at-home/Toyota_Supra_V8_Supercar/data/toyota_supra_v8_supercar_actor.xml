                    <node name="TOY_SUP_GT4_EVO_BUMPER_F_ALPHA"/>
                    <node name="TOY_SUP_GT4_EVO_BUMPER_F_GLASS"/>
                    <cpit name="TOY_SUP_GT4_EVO_BUMPER_F_CPIT"/>
