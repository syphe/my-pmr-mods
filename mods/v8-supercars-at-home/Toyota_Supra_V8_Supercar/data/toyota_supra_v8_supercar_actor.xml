                        <widget name="shift_lights2" type="shiftlights" bindingId="TOY_SUP_GT4_EVO_DISPLAY_LIGHT_RPM_02" sParam="0_3_0" r="0.3" g="0.3" b="0.3" amberT="2/4" vehicleParam="revs"/>
